#ifndef _XENOMAI_VXWORKS_TICKLIB_H
#define _XENOMAI_VXWORKS_TICKLIB_H

#include <vxworks/types.h>

#ifdef __cplusplus
extern "C" {
#endif

ULONG tickGet(void);

void tickSet(ULONG ticks);

#ifdef __cplusplus
}
#endif

