ART = (1 &lt;&lt; 0),</div><div class="line"><a name="l00315"></a><span class="lineno">  315</span>&#160;        CHOR_PON = (CHOR_CLR_SEND_TC | CHOR_CLR_LPAUSE),</div><div class="line"><a name="l00316"></a><span class="lineno">  316</span>&#160;};</div><div class="line"><a name="l00317"></a><span class="lineno">  317</span>&#160;</div><div class="line"><a name="l00318"></a><span class="lineno">  318</span>&#160;<span class="keyword">enum</span> MITE_CHCR_bits {</div><div class="line"><a name="l00319"></a><span class="lineno">  319</span>&#160;        CHCR_SET_DMA_IE = (1 &lt;&lt; 31),</div><div class="line"><a name="l00320"></a><span class="lineno">  320</span>&#160;        CHCR_CLR_DMA_IE = (1 &lt;&lt; 30),</div><div class="line"><a name="l00321"></a><span class="lineno">  321</span>&#160;        CHCR_SET_LINKP_IE = (1 &lt;&lt; 29),</div><div class="line"><a name="l00322"></a><span class="lineno">  322</span>&#160;        CHCR_CLR_LINKP_IE = (1 &lt;&lt; 28),</div><div class="line"><a name="l00323"></a><span class="lineno">  323</span>&#160;        CHCR_SET_SAR_IE = (1 &lt;&lt; 27),</div><div class="line"><a name="l00324"></a><span class="lineno">  324</span>&#160;        CHCR_CLR_SAR_IE = (1 &lt;&lt; 26),</div><div class="line"><a name="l00325"></a><span class="lineno">  325</span>&#160;        CHCR_SET_DONE_IE = (1 &lt;&lt; 25),</div><div class="line"><a name="l00326"></a><span class="lineno">  326</span>&#160;        CHCR_CLR_DONE_IE = (1 &lt;&lt; 24),</div><div class="line"><a name="l00327"></a><span class="lineno">  327</span>&#160;        CHCR_SET_MRDY_IE = (1 &lt;&lt; 23),</div><div class="line"><a name="l00328"></a><span class="lineno">  328</span>&#160;        CHCR_CLR_MRDY_IE = (1 &lt;&lt; 22),</div><div class="line"><a name="l00329"></a><span class="lineno">  329</span>&#160;        CHCR_SET_DRDY_IE = (1 &lt;&lt; 21),</div><div class="line"><a name="l00330"></a><span class="lineno">  330</span>&#160;        CHCR_CLR_DRDY_IE = (1 &lt;&lt; 20),</div><div cl