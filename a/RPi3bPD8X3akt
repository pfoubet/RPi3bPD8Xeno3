category  "ts_ZA:2004";LC_MESSAGES
category  "ts_ZA:2004";LC_PAPER
category  "ts_ZA:2004";LC_NAME
category  "ts_ZA:2004";LC_ADDRESS
category  "ts_ZA:2004";LC_TELEPHONE
