partial alphanumeric_keys
xkb_symbols "olpc" {
    include "am(basic)"
    name[Group1]= "Armenian";

    include "group(olpc)"
};

xkb_symbols "phonetic" {
    include "am(basic)"
    name[Group1]= "Armenian (phonetic)";

    key <BKSL> {	[ 0x1000577,  0x1000547 ]	};

