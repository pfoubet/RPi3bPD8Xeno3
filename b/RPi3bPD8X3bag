CTL{numid}="987"
CTL{iface}="sequencer"
CTL{device}="10"
CTL{subdevice}="20"
CTL{name}="Just Test"
CTL{index}="999"

PRINT="After test sequence:\n"
PRINT="  CTL{numid}=\"$ctl{numid}\"\n"
PRINT="  CTL{iface}=\"$ctl{iface}\"\n"
