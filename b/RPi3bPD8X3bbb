PERL_CALLCONV OP *Perl_pp_truncate(pTHX);
PERL_CALLCONV OP *Perl_pp_uc(pTHX);
PERL_CALLCONV OP *Perl_pp_ucfirst(pTHX);
PERL_CALLCONV OP *Perl_pp_umask(pTHX);
