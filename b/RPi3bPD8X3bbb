# Abort on error.
set -e

PKG=e2fsprogs
DOCLNK=/usr/share/doc/$PKG
if test "$1" = upgrade \
    -a -L $DOCLNK
then
    rm $DOCLNK
fi

# dh_installdeb will replace this with shell code automatically
