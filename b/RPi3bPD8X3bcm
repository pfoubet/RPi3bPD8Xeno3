"><a name="l00151"></a><span class="lineno">  151</span>&#160;        FullDuplex              = (1 &lt;&lt; 9),</div><div class="line"><a name="l00152"></a><span class="lineno">  152</span>&#160;        TxOn                    = 0x2000,</div><div class="line"><a name="l00153"></a><span class="lineno">  153</span>&#160;        AcceptBroadcast         = 0x0100,</div><div class="line"><a name="l00154"></a><span class="lineno">  154</span>&#160;        AcceptAllMulticast      = 0x0080,</div><div class="line"><a name="l00155"></a><span class="lineno">  155</span>&#160;        AcceptAllPhys           = 0x0040,</div><div class="line"><a name="l00156"></a><span class="lineno">  156</span>&#160;        AcceptRunt              = 0x0008,</div><div class="line"><a name="l00157"></a><span class="lineno">  157</span>&#160;        RxOn                    = 0x0002,</div><div class="line"><a name="l00158"></a><span class="lineno">  158</span>&#160;        RxTx                    = (TxOn | RxOn),</div><div class="line"><a name="l00159"></a><span class="lineno">  159</span>&#160;};</div><div class="line"><a name="l00160"></a><span class="lineno">  160</span>&#160;</div><div class="line"><a name="l00161"></a><span class="lineno">  161</span>&#160;</div><div class="line"><a name="l00162"></a><span class="lineno">  162</span>&#160;<span class="keyword">enum</span> tulip_busconfig_bits {</div><div class="line"><a name="l00163"></a><span class="lineno">  163</span>&#160;        MWI                     = (1 &lt;&lt; 24),</div><div class="line"><a name="l00164"></a><span class="lineno">  164</span>&#160;        MRL                     = (1 &lt;&lt; 23),</div><div class="line"><a name="l00165"></a><span class="lineno">  165</span>&#160;        MRM                     = (1 &lt;&lt; 21),</div><div class="line"><a name="l00166"></a><span class="lineno">  166</span>&#160;        CALShift                = 14,</div><div class="line"><a name="l00167"></a><span class="lineno">  167</span>&#160;      