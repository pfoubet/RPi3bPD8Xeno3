			(yyvsp[0].node)->is_root = 1;
			the_boot_info = build_boot_info((yyvsp[-1].re), (yyvsp[0].node),
							guess_boot_cpuid((yyvsp[0].node)));
		}
#line 1477 "dtc-parser.tab.c" /* yacc.c:1646  */
    break;

  case 3:
