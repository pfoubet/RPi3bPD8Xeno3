#define CONFIG_XENO_BUILD_STRING "x86_64-unknown-linux-gnu"

/* config */
#define CONFIG_XENO_COBALT 1

/* Compiler */
#define CONFIG_XENO_COMPILER "gcc version 4.8.4 (Ubuntu/Linaro 4.8.4-2ubuntu1~14.04.1) "

