ority(<span class="keywordtype">int</span> prio)</div><div class="line"><a name="l00072"></a><span class="lineno">   72</span>&#160;{                               <span class="comment">/* FIXME: HIPRIO is lower over Mercury */</span></div><div class="line"><a name="l00073"></a><span class="lineno">   73</span>&#160;        <span class="keywordflow">return</span> prio &lt; T_LOPRIO || prio &gt; T_HIPRIO ? -EINVAL : 0;</div><div class="line"><a name="l00074"></a><span class="lineno">   74</span>&#160;}</div><div class="line"><a name="l00075"></a><span class="lineno">   75</span>&#160;</div><div class="line"><a name="l00076"></a><span class="lineno">   76</span>&#160;<span class="keyword">extern</span> <span class="keyword">struct </span>syncluster alchemy_task_table;</div><div class="line"><a name="l00077"></a><span class="lineno">   77</span>&#160;</div><div class="line"><a name="l00078"></a><span class="lineno">   78</span>&#160;<span class="preprocessor">#endif </span><span class="comment">/* _ALCHEMY_TASK_H */</span><span class="preprocessor"></span></div></div><!-- fragment --></div><!-- contents -->
