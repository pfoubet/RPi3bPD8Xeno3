#ifndef _XENOMAI_TRANK_NATIVE_HEAP_H
#define _XENOMAI_TRANK_NATIVE_HEAP_H

#include <alchemy/heap.h>

#define H_MAPPABLE  0
#define H_SHARED    0
#define H_NONCACHED 0
#define H_DMA	    0

