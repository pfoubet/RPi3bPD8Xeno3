<confdir:pcm/surround40.conf>

ICE1724.pcm.surround40.0 {
	@args [ CARD ]
	@args.CARD {
		type string
	}
	type route
	ttable.0.0 1
	ttable.1.1 1
	ttable.2.4 1
	ttable.3.5 1
	slave {
		channels 6
		pcm {
			type hw
			card $CARD
		}
	}
}	

